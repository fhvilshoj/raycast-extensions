{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "internal-dictionary",
  "title": "Internal Dictionary",
  "description": "Build an internal dictionary of words or achronyms and (fuzzy) search them.",
  "icon": "command-icon.png",
  "author": "frederik_hvilshoj",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Internal Lookup",
      "description": "Show Type ahead search results",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@raycast/api": "^1.56.3",
    "@raycast/utils": "^1.4.8",
    "fuse.js": "^6.6.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
